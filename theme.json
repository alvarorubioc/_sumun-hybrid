				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontWeight": true,
			"letterSpacing": false,
			"lineHeight": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "BW Nista",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/BwNistaInt-Md.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "BW Nista",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/BwNistaInt-Lt.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "BW Nista",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "bold",
							"src": [
								"file:./assets/fonts/BwNistaInt-xBd.woff2"
							]
						}
					],
					"fontFamily": "\"BW Nista\", sans-serif",
					"name": "Font Main",
					"slug": "font-main"
				}
			],
			"fontSizes": [
				{
					"name": "Display",
					"size": "clamp(3rem, 8.8vw, 8rem)",
					"slug": "display"
				},
				{
					"name": "H1",
					"size": "clamp(3rem, 4.8vw, 4rem)",
					"slug": "heading-1"
				},
				{
					"name": "H2",
					"size": "clamp(2rem, 4.4vw, 3rem)",
