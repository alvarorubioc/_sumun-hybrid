		"custom": {
			"border-radius": {
				"0": "0",
				"025": "4px",
				"05": "8px",
				"1": "16px",
				"1-5": "24px",
				"2": "32px",
				"rounded": "999px"
			},
			"box-shadow": {
				"1": "0px 2px 8px rgba(33, 33, 33, 0.12)",
				"2": "3px 3px 10px 10px rgba(0, 0, 0, 0.10)"
			},
			"layout": {
				"gap": "2rem",
				"gap-large": "40px",
				"content": "1216px",
				"content-wide": "90%",
				"padding": "16px"
			},
			"line-height": {
				"headings": 1.2,
				"normal": 1.5,
				"tiny": 1
			}
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content)",
			"wideSize": "var(--wp--custom--layout--wide)"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Space 1 (1rem - 16px)",
					"size": "1rem",
					"slug": "10"
				},
				{
					"name": "Space 2 (1.5rem - 24px)",
					"size": "1.5rem",
